<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<GridPane fx:controller="GUI"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <Button text="Create Tree" onAction="#createTree" GridPane.rowIndex="0"/>
    <Button text=
                    "Compare" onAction=
                    "#compare"
                GridPane.rowIndex="2"/>
    <TextField text=""
               GridPane.rowIndex="3"
               fx:id="input"/>
    <Label GridPane.rowIndex=
                   "4" fx:id=
                   "result"/>
    <Label GridPane.rowIndex=
                   "5" fx:id=
                   "closest"/>
    <Label GridPane.rowIndex="6" fx:id = "mediod"/>
    <Label GridPane.rowIndex="7" fx:id = "mediodres"/>
    <Button text=
                    "Shortest Path" onAction=
                    "#createGraph"
            GridPane.rowIndex="8"/>
    <Label GridPane.rowIndex="9" fx:id = "shortestres"/>
    <Label GridPane.rowIndex="10" fx:id = "spanningTrees"/>
    <Label GridPane.rowIndex="11" fx:id = "cost"/>



</GridPane>